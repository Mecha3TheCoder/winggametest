			self.global_position = self.global_position - Vector2(100,0)
		if event.is_action(\"ui_right\"):
			self.global_position = self.global_position + Vector2(100,0)
		if event.is_action(\"ui_up\"):
			self.global_position = self.global_position - Vector2(0,100)
		if event.is_action(\"ui_down\"):
			self.global_position = self.global_position + Vector2(0,100)



